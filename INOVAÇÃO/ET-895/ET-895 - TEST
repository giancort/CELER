SELECT TOP 1
	*
FROM VW_COMPANY_EC_AFF_BR_DEP_EQUIP vceabde
ORDER BY 1 DESC

SELECT TOP 5
	*
FROM ASS_DEPTO_EQUIP ade
ORDER BY 1 DESC

SELECT
	*
FROM PRODUCTS_ACQUIRER pa
WHERE COD_BRAND = 1

EXEC [SP_VALIDATE_TRANSACTION] @TERMINALID = 28862
							  ,@TYPETRANSACTION = 'DEBIT'
							  ,@AMOUNT = 100
							  ,@QTY_PLOTS = 1
							  ,@PAN = '64***6565'
							  ,@BRAND = 'MAESTRO'
							  ,@TRCODE = '1593107910104525657003'
							  ,@TERMINALDATE = NULL
							  ,@CODPROD_ACQ = 2
							  ,@TYPE = 'TRANSACTION'
							  ,@COD_BRANCH = 10
							  ,@CODE_SPLIT = NULL
							  ,@COD_EC = 14077
							  ,@HOLDER_NAME = 'KENNEDY ALEF DE OLIVEIRA'
							  ,@HOLDER_DOC = NULL
							  ,@LOGICAL_NUMBER = NULL
							  ,@COD_TRAN_PROD = NULL
							  ,@COD_EC_PRD = NULL



GO



EXEC SP_UP_TRANSACTION @CODE_TRAN = '1593107910104525657003'
					  ,@SITUATION = 'APPROVED'
					  ,@DESCRIPTION = '100-APROVADA'
					  ,@CURRENCY = NULL
					  ,@CODE_ERROR = NULL
					  ,@TRAN_ID = 4798372
					  ,@LOGICAL_NUMBER_ACQ = NULL
					  ,@CARD_HOLDER_NAME = ''
					  ,@COD_USER = NULL
GO
EXEC SP_UP_TRANSACTION @CODE_TRAN = '1593107910104525657003'
					  ,@SITUATION = 'CONFIRMED'
					  ,@DESCRIPTION = '200-CONFIRMADA'
					  ,@CURRENCY = NULL
